{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 20,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Information",
      "Function": "Information",
      "Host.Aggregator": "Information"
    }
  },
  "singleton": {
    "lockPeriod": "00:00:15",
    "listenerLockPeriod": "00:00:15",
    "listenerLockRecoveryPollingInterval": "00:00:15",
    "lockAcquisitionTimeout": "00:00:15",
    "lockAcquisitionPollingInterval": "00:00:05"
  }
}
